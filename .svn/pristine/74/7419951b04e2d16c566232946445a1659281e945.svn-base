  height: 130px;
}
.opcs-top .opcs-mem-info-bd .opcs-photo .opcs-img-bg {
  display: block;
  width: 100px;
  height: 100px;
  text-align: center;
  margin: 15px auto 0 auto;
  overflow: hidden;
  background: url("../Images/photo.png") no-repeat center center;
  position: relative;
}
.opcs-top .opcs-mem-info-bd .opcs-photo .opcs-img-bg>span{
  position: absolute;
  bottom: 3px;
  color: #fff;
  display: block;
  width: 100%;
  left: 0;
}
.opcs-top .opcs-mem-info-bd .opcs-photo .opcs-img-bg img {
  width: 77px;
  height: 77px;
  margin-top: 4px;
  border-radius: 100%;
}
.opcs-top .opcs-mem-info-bd .opcs-info {
  width: 522px;
}
.opcs-top .opcs-mem-info-bd .opcs-info .opcs-row1 {
  margin-top:29px;
  height: 30px;
}
.opcs-top .opcs-mem-info-bd .opcs-info .opcs-row2 {
  color: #000;
  font-size: 12px;
  height: 20px;
  line-height: 20px;
}
.opcs-top .opcs-mem-info-bd .opcs-info .opcs-row3 {
  color: #7f7f7f;
  font-size: 9px;
  line-height: 20px;
}

.opcs-top .opcs-mem-info-bd .opcs-info .opcs-row1 .opcs-hello {
  color: #ec5051;
  font-size: 18px;
}
.opcs-top .opcs-mem-info-bd .opcs-info .opcs-row2 span {
  padding-right: 20px;
}
.opcs-top .opcs-mem-info-bd .opcs-info .opcs-row2 span {
  padding-right: 20px;
}
.opcs-top .opcs-mem-info-bd .opcs-info .opcs-row3 {
  color: #7f7f7f;
  font-size: 9px;
  line-height: 20px;
}
.opcs-info2 ul{
  margin-top: 50px;
}
.opcs-top .opcs-mem-info-bd .opcs-info2 ul li {
  list-style: none;
  float: left;
  height: 55px;
  text-align: center;
  margin-right: 6px;
}
.opcs-top .opcs-mem-info-bd .opcs-info2 ul li div {
  line-height: 28px;
