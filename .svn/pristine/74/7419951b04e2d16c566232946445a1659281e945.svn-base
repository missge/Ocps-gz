/*首页header*/
.opsc-pc-header{
  height: 131px;
  background: url(../Images/banner.png) center center no-repeat;
  background-size: cover;
  border-bottom: 8px solid #A9D8F6;
}
.opsc-pc-banner-bd{
  height: 131px;
  width: 1210px;
  margin: 0 auto;
  position: relative;
}
.opsc-pc-header .opsc-pc-banner-bd .opsc-pc-top-opts {
  text-align: right;
  margin-right: 15px;
  font-size: 12px;
  padding-top: 10px;
}
.opsc-pc-header .opsc-pc-banner-bd .opsc-pc-logo {
  position: absolute;
  top: 25px;
  left: 30px;
  width: 132px;
  height: 88px;
  display: block;

}
.opsc-pc-header .opsc-pc-banner-bd .opsc-pc-logo img {
  width: 132px;
  height: 88px;
}
.opsc-pc-header .opsc-pc-banner-bd .opsc-pc-top-opts a {
  color: #0b8ee9;
}

.opsc-pc-header .opsc-pc-banner-bd .opsc-pc-home-title {
  display: block;
  width: 196px;
  height: 41px;
  background: url(../Images/membercenter.png) center center no-repeat;
  position: absolute;
  left: 190px;
  top: 55px;
}
.opsc-pc-header .opsc-pc-banner-bd .opsc-pc-other-info {
   position: absolute;
   top: 55px;
   right: 15px;
   height: 40px;
   line-height: 40px;
   color: #333;
   font-size: 16px;
 }
.opsc-pc-header .opsc-pc-banner-bd .opsc-pc-other-info span {
  font-family: Arial;
  font-weight: bold;
}

/*nav*/
.opcs-top {
  /*background: #e7e7e7;*/
  height: 130px;
}
.opcs-top .opcs-mem-info-bd {
  height: 130px;
  width: 1210px;
  margin: 0 auto;
  position: relative;
}
.opcs-top .opcs-mem-info-bd .opcs-photo {
  width: 140px;
}
.opcs-top .opcs-mem-info-bd>div {
  display: inline-block;
  zoom: 1;
